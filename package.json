{
  "name": "split-page-numbers",
  "version": "0.0.0",
  "description": "Stateless page number generator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha --extension ts --recursive --require ts-node/register src",
    "test:update": "SNAPSHOT_UPDATE=1 npm test",
    "coverage": "nyc -r lcov -e .ts -x '**/*.test.ts' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roccivic/split-page-numbers.git"
  },
  "keywords": [
    "page-numbers",
    "page-range",
    "pager",
    "pagination",
    "paginate",
    "paginator",
    "paging",
    "navigation"
  ],
  "author": "Rouslan Placella <rouslan@placella.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roccivic/split-page-numbers/issues"
  },
  "homepage": "https://github.com/roccivic/split-page-numbers#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "mocha": "^9.1.0",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "snap-shot-it": "^7.9.6",
    "ts-mock-imports": "^1.3.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
